pipeline {
    agent none
    stages {
        stage('closeGithubIssue') {
            steps {
                script {
                    closeGithubIssue(
                        repoUrl: 'https://github.com/opensearch-project/opensearch-build',
                        issueTitle: 'Test GH issue title',
                        closeComment: 'Test GH issue close comment',
                        label: 'label101'
                        )
                }
            }
        }
    }
}
