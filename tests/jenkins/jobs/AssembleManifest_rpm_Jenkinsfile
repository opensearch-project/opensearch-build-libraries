lib = library(identifier: "jenkins@1.0.0", retriever: legacySCM(scm))

pipeline {
    agent none
    stages {
        stage('assembleManifest rpm') {
            steps {
                script {
                    assembleManifest(
                        buildManifest: 'tests/data/opensearch-build-1.3.0-rpm.yml'
                    )
                }
            }
        }
    }
}