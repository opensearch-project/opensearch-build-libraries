pipeline {
    agent none
    stages {
        stage('createGithubIssue') {
            steps {
                script {
                    createGithubIssue(
                        repoUrl: "https://github.com/opensearch-project/opensearch-build",
                        issueTitle: "Test GH issue title",
                        issueBody: "Test GH issue body",
                        label: "label101",
                        daysToReOpen: "5"
                        )
                }
            }
        }
    }
}
