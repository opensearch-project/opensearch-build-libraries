lib = library(identifier: "jenkins@1.0.0", retriever: legacySCM(scm))

pipeline {
    agent none
    stages {
        stage('buildYumRepo') {
            steps {
                script {
                    buildYumRepo(
                        buildManifest: 'tests/data/opensearch-build-1.3.0.yml',
                        baseUrl: 'https://ci.opensearch.org/ci/dbc/test/1.3.0/9/linux/x64'
                    )
                }
            }
        }
    }
}