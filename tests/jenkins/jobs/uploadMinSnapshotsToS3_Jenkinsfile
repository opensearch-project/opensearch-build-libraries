pipeline {
    agent none
    stages {
        stage('uploadMinSnapshotsToS3') {
            steps {
                script {
                    uploadMinSnapshotsToS3(
                        fileActions: [createSha512Checksums()],
                        manifest: 'tests/data/opensearch-1.3.0-build.yml',
                        distribution: 'tar'
                    )
                }
            }
        }
    }
}